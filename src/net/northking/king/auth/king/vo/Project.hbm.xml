<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 映射持久化对象与数据库表的映射关联 -->
<hibernate-mapping>
	<class name="net.northking.king.auth.king.vo.Project" table="TB_PROJECT">
		<id name="id" type="string">
			<column name="ID"></column>
		</id>
		<property name="name" column="PROJECT_NAME" type="string"></property>
		<property name="num" column="NUM" type="string"></property>
		<property name="address" column="ADDRESS" type="string"></property>
		<property name="startDate" column="START_DATE" type="date"></property>
		<property name="endDate" column="END_DATE" type="date"></property>
		<property name="contractBudget" column="CONTRACT_MONEY" type="string"></property>
	</class>
</hibernate-mapping>
